    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.5.7",
    "express": "^4.16.4",
    "inversify": "^5.0.1",
    "inversify-binding-decorators": "^4.0.0",
    "inversify-inject-decorators": "^3.1.0",
    "inversify-logger-middleware": "^3.1.0",
    "multer": "^1.4.1",
    "ts-loader": "^5.2.2",
    "typescript": "^3.1.3",
    "webpack": "^4.23.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.0",
